<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FileManager Plugin wx3.1.x - MSys2" />
		<Option platforms="Windows;" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel31/share/CodeBlocks/plugins/FileManager" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs31/plugins/contrib/FileManager" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_aui-$(WX_VERSION).dll" />
					<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_xrc-$(WX_VERSION).dll" />
					<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
					<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
					<Add directory="../../../devel31" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_TOOLKIT" value="msw" />
				<Variable name="WX_VERSION" value="3.1" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-std=gnu++11" />
			<Add option="-Winvalid-pch" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-D__WXMSW__" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="../../../.objs31/include" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../include/mozilla_chardet" />
			<Add directory="../../../include/mozilla_chardet/mfbt" />
			<Add directory="../../../include/mozilla_chardet/nsprpub/pr/include" />
			<Add directory="../../../include/mozilla_chardet/xpcom" />
			<Add directory="../../../include/mozilla_chardet/xpcom/base" />
			<Add directory="../../../include/mozilla_chardet/xpcom/glue" />
			<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/$(WX_TOOLKIT)-unicode-$(WX_VERSION)" />
			<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
		</Compiler>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel31\share\codeblocks\FileManager.zip manifest.xml Resources\fileexplorersettings.xrc Resources\CommitBrowser.xrc" />
		</ExtraCommands>
		<Unit filename="CommitBrowser.cpp" />
		<Unit filename="CommitBrowser.h" />
		<Unit filename="FileExplorer.cpp" />
		<Unit filename="FileExplorer.h" />
		<Unit filename="FileExplorerSettings.cpp" />
		<Unit filename="FileExplorerSettings.h" />
		<Unit filename="FileExplorerUpdater.cpp" />
		<Unit filename="FileExplorerUpdater.h" />
		<Unit filename="FileManager.cpp" />
		<Unit filename="FileManager.h" />
		<Unit filename="Resources/CommitBrowser.xrc" />
		<Unit filename="Resources/fileexplorersettings.fbp" />
		<Unit filename="Resources/fileexplorersettings.xrc" />
		<Unit filename="directorymonitor.cpp" />
		<Unit filename="directorymonitor.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
